{
    "name": "moshpit",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://codewithmosh.com/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Moshpit"
    },
    "permissions": ["tabs", "downloads"]
}